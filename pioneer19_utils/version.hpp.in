#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define PIONEER19_UTILS_VERSION       $pioneer19_utils.version.project_number$ULL
#define PIONEER19_UTILS_VERSION_STR   "$pioneer19_utils.version.project$"
#define PIONEER19_UTILS_VERSION_ID    "$pioneer19_utils.version.project_id$"

#define PIONEER19_UTILS_VERSION_MAJOR $pioneer19_utils.version.major$
#define PIONEER19_UTILS_VERSION_MINOR $pioneer19_utils.version.minor$
#define PIONEER19_UTILS_VERSION_PATCH $pioneer19_utils.version.patch$

#define PIONEER19_UTILS_PRE_RELEASE   $pioneer19_utils.version.pre_release$

#define PIONEER19_UTILS_SNAPSHOT_SN   $pioneer19_utils.version.snapshot_sn$ULL
#define PIONEER19_UTILS_SNAPSHOT_ID   "$pioneer19_utils.version.snapshot_id$"
